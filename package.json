    "@rocketseat/unform": "^1.5.1",
    "axios": "^0.19.0",
    "dotenv": "^8.0.0",
    "moment": "^2.29.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.0",
    "react-select": "^3.0.4",
